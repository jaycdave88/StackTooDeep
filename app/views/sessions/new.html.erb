<div class="col-md-6 col-md-offset-3 text-center">
  <%= render "shared/nav" %>
  <h1>Log In</h1>

  <%= form_tag sessions_path do %>
      <%= label_tag :email %><br>
      <%= text_field_tag :email, params[:email] %><br>
      <%= label_tag :password %><br>
      <%= password_field_tag :password %><br>
      <%= submit_tag "Log In", class: "btn btn-default" %>
  <%end%>
</div>
